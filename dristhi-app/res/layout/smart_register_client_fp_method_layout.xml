<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="@dimen/list_fp_view_width"
        android:layout_height="fill_parent"
        android:layout_weight="@integer/ec_list_fp_view_weight"
        android:layout_marginTop="5dp"
        android:orientation="vertical">

    <TextView
            android:id="@+id/txt_fp_method"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.ListView.Item.Medium"/>

    <TextView
            android:id="@+id/txt_fp_method_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.ListView.Item.Small"/>

    <TextView
            android:id="@+id/txt_fp_method_quantity_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.ListView.Item.Small"
            android:text="@string/str_number_of_strips"/>

    <TextView
            android:id="@+id/txt_fp_method_quantity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.ListView.Item.Small"/>

    <TextView
            android:id="@+id/txt_iud_place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.ListView.Item.Small"/>

    <TextView
            android:id="@+id/txt_iud_place_person_separator"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.ListView.Item.Small"
            android:text="@string/str_comma_separator"/>

    <TextView
            android:id="@+id/txt_iud_person"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.ListView.Item.Small"/>

</LinearLayout>

    