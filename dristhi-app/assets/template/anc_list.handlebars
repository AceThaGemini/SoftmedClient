{{#each this}}
<div class="anc {{villageName}} client-list-item" data-caseId={{caseId}}>
    <div class="row-fluid" onClick="javascript: return true;">
        <div class="span8">
            <ul class="client-details unstyled">
                <li class="big-text">
                    {{capitalize womanName}}
                    {{#if isHighRisk}}<span class="high-risk"> HRP </span>{{/if}}
                </li>
                <li>{{capitalize husbandName}}</li>
                <li>
                    Thayi No: {{thaayiCardNumber}}{{#if ecNumber}}, EC No: {{ecNumber}}{{/if}}
                </li>
            </ul>
        </div>

        <div class="span4 pull-text-completely-right">
            {{formatText villageName}}
            <br>
            {{#if hasTodos}}
            <div class="todo pull-text-completely-right">
                <img src="../img/icons/icon-hastodo.png">
            </div>
            {{/if}}
        </div>
    </div>

    <div class="divider divider-padding-bottom"></div>
</div>
{{/each}}
